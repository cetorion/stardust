{
	"name": "stardust",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"serve": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"lint:c": "eslint .",
		"lint:w": "eslint --fix .",
		"fmt:w": "prettier --write \"src/**/*.{ts,tsx}\"",
		"fmt:c": "prettier --check \"src/**/*.{ts,tsx}\"",
		"reset": "rm -rf dist node_modules yarn.lock && yarn cache clean && yarn install",
		"upset": "yarn cache clean && yarn upgrade -L"
	},
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.10.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react-swc": "^4.2.2",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.4"
	}
}
